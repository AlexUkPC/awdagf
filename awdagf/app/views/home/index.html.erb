<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

 <% if current_user %>
    <%= current_user.email %>
    <%= render "action_button",
    caption: "Sign out",
    link: destroy_user_session_path,
    method: :delete %>
<% else %>
    <%= render "action_button",
    caption: "Sign in",
    link: new_user_session_path %>
<% end %>